/*FIXME_phlat_QMenu.qss*/

.QMenu {
background:palette(window);
border: 1px solid rgba(0,0,0,0.8);}
.QMenu::item{
background-color: transparent;}
.QMenu::item:selected {
background:palette(highlight);}
.QMenu::separator {
height:1px;
background: palette(window);
border-top:1px solid rgba(0,0,0,0.8);}

/*phlat_QToolBar.qss*/

QToolBar {
background-color: palette(window);
color: palette(window-text);
border: 1px solid transparent;}
.QToolBar::handle:horizontal{
image: url(/usr/share/themes/0nyX/images/gtk-2.0/handle_vertical.svg);}
.QToolBar::handle:vertical{
image: url(/usr/share/themes/0nyX/images/gtk-2.0/handle_horizontal.svg);}
.QToolBar::separator:horizontal{
margin:2px;
background:transparent}

/*phlat_QMenuBar.qss*/

.QMenuBar{
padding:0px;
margin:0px;
border:0px;
font-weight:bold;}
.QMenuBar::item,
.QMenuBar::item:selected {
border:0px;
padding:2px 3px;
padding-bottom:6px;
color:palette(highlight-text);}
.QMenuBar::item:disabled {
color:#656666;}
.QMenuBar::item:pressed {
margin-bottom:2px;
color:palette(highlight);
background:rgba(0,0,0,0.4);}

/*phlat_QLineEdit.qss*/

QLineEdit {
padding:1px;
color:palette(text);
background:palette(base);
border:1px solid rgba(0,0,0,0.6);
selection-background-color:palette(highlight);
selection-color:palette(highlight-text);}
QLineEdit:disabled {
color:#656666;}

/*phlat_QStackedWidget.qss*/

.QStackedWidget {
border:0px;}
/*phlat_QProgressBar.qss*/

.QProgressBar {
