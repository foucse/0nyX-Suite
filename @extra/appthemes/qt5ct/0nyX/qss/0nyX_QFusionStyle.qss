/*phlat_QScrollBars.qss*/

.QScrollBar:horizontal {
background: rgba(0,0,0,0.3);
border:1px solid rgba(0,0,0,0.8);
border-width: 1px 0px 1px 0px;
height: 18px;
margin: 0px 18px 0 18px;}
.QScrollBar:vertical {
background: rgba(0,0,0,0.3);
border:1px solid rgba(0,0,0,0.8);
border-width: 0px 1px 0px 1px;
width: 18px;
margin: 18px 0px 18px 0px;}
.QScrollBar::handle:horizontal {
border:1px solid  rgba(0,0,0,0.8);
background: palette(highlight);
min-width: 35px;
margin:-1px 0px;}
.QScrollBar::handle:vertical {
border:1px solid  rgba(0,0,0,0.8);
background: palette(highlight);
min-height: 35px;
margin:0px -1px;}
.QScrollBar::add-page,
.QScrollBar::sub-page {
background: none;}
.QScrollBar::add-line:horizontal {
border:solid rgba(0,0,0,0.6);
border-width:1px 1px 1px 0px;
width: 17px;
subcontrol-position: right;
subcontrol-origin: margin;}
.QScrollBar::sub-line:horizontal {
border:solid rgba(0,0,0,0.6);
border-width:1px 0px 1px 1px;
width: 17px;
subcontrol-position: left;
subcontrol-origin: margin;}
.QScrollBar::add-line:vertical {
border:solid rgba(0,0,0,0.6);
border-width:0px 1px 1px 1px;
height: 17px;
subcontrol-position: bottom;
subcontrol-origin: margin;}
.QScrollBar::sub-line:vertical {
border:solid rgba(0,0,0,0.6);
border-width:1px 1px 0px 1px;
height: 17px;
subcontrol-position: top;
subcontrol-origin: margin;}
.QScrollBar::up-arrow:vertical{
width: 17px;
height: 17px;
background: rgba(0,0,0,0.3);
background-position: center; 
background-repeat: no-repeat;
background-image: url("/usr/share/themes/0nyX/images/gtk-2.0/arrow-up_normal.svg");}
.QScrollBar::down-arrow:vertical{
width: 17px;
height: 17px;
background: rgba(0,0,0,0.3);
background-position: center; 
background-repeat: no-repeat;
background-image: url("/usr/share/themes/0nyX/images/gtk-2.0/arrow-down_normal.svg");}
.QScrollBar::left-arrow:horizontal{
width: 17px;
height: 17px;
background: rgba(0,0,0,0.3);
background-position: center; 
background-repeat: no-repeat;
background-image: url("/usr/share/themes/0nyX/images/gtk-2.0/arrow-left_normal.svg");}
.QScrollBar::right-arrow:horizontal{
width: 17px;
height: 17px;
background: rgba(0,0,0,0.3);
background-position: center; 
background-repeat: no-repeat;
background-image: url("/usr/share/themes/0nyX/images/gtk-2.0/arrow-right_normal.svg");}

QListView .QScrollBar:horizontal,
QTreeWidget .QScrollBar:horizontal,
QTableWidget .QScrollBar:horizontal,
QMdiArea .QScrollBar:horizontal,
QGraphicsView .QScrollBar:horizontal,
QTextEdit .QScrollBar:horizontal,
QPlainTextEdit.QScrollBar:horizontal,
QIconView .QScrollBar:horizontal{
border-width:1px 0px 0px 0px;}

QListView .QScrollBar:vertical,
QTreeWidget .QScrollBar:vertical,
QTableWidget .QScrollBar:vertical,
QMdiArea .QScrollBar:vertical,
QGraphicsView .QScrollBar:vertical,
QTextEdit .QScrollBar:vertical,
QPlainTextEdit.QScrollBar:vertical,
