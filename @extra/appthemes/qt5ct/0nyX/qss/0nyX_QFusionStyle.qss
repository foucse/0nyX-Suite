/*phlat_QDockWidget.qss*/

QDockWidget {
border:1px solid rgba(0,0,0,0.8);
titlebar-close-icon: url(/usr/share/themes/0nyX/images/close.svg);
titlebar-normal-icon:  url(/usr/share/themes/0nyX/images/restore.svg);}
QDockWidget::title {
text-align: center;
font-weight:bold;
margin:-1px;
border:1px solid rgba(0,0,0,0.8);
background-color: palette(window);}
QDockWidget::close-button, 
QDockWidget::float-button {
border:0px;
padding:0px;
icon-size: 22px; }
QDockWidget::close-button:pressed{
border:0px;
background:#cc0000}
QDockWidget::float-button:pressed{
border:0px;
background:palette(highlight);}
/*phlat_QTabWidget.qss*/

.QTabWidget::tab-bar {
alignment: center;}

.QTabWidget::pane {
margin-top:-1px;
border-style: solid;
border-width: 1px 1px 1px 1px;
border-color:rgba(0,0,0,0.8);}

.QTabBar::tab {
padding:2px 15px;
border:1px solid  rgba(0,0,0,0.8);
background-color:palette(base);}
QTabBar::tab:selected {
border:1px solid  rgba(0,0,0,0.8);
background-color:palette(highlight);}

.QTabBar::tab:top:!first,
.QTabBar::tab:bottom:!first {
padding:4px 4px;
margin-left: -1;}

.QTabBar::tab:left:!first,
.QTabBar::tab:right:!first {
padding:4px 4px;
margin-top: -1;}

.QTabBar::tear {
background:palette(highlight);
border:1px solid  rgba(0,0,0,0.8);}
.QTabBar QToolButton {
padding:5px;
border:1px solid  rgba(0,0,0,0.8);
background:palette(base);}
.QTabBar::close-button {}


/*phlat_QSlider.qss*/

.QSlider::groove:horizontal {
border:1px solid;
background:palette(base);
height:3px;}
.QSlider::handle:horizontal {
margin:-3px -1px;
width:14px;
background:palette(highlight);
border:1px solid;}
.QSlider::groove:vertical {
border:1px solid;
background:palette(base);
width:3px;}
.QSlider::handle:vertical {
margin:-1px -3px;
height:14px;
background:palette(highlight);
border:1px solid}
.QSlider::sub-page:horizontal,
.QSlider::add-page:vertical {
background:palette(highlight);
border:1px solid}
.QSlider::sub-page:horizontal:disabled,
.QSlider::add-page:vertical:disabled,
.QSlider::handle:disabled{
background:palette(window);}
