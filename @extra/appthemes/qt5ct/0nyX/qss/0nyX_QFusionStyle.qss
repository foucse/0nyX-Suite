QTextEdit .QScrollBar::sub-line:horizontal,
QPlainTextEdit.QScrollBar::sub-line:horizontal,
QIconView .QScrollBar::sub-line:horizontal {
border:0px;
border-top:1px solid rgba(0,0,0,0.8);
border-right:1px solid rgba(0,0,0,0.3);
height: 17px;}

QListView .QScrollBar::add-line:vertical
QTreeWidget .QScrollBar::add-line:vertical,
QTableWidget .QScrollBar::add-line:vertical,
QMdiArea .QScrollBar::add-line:vertical,
QGraphicsView .QScrollBar::add-line:vertical,
QTextEdit .QScrollBar::add-line:vertical,
QPlainTextEdit.QScrollBar::add-line:vertical,
QIconView .QScrollBar::add-line:vertical {
border:0px;
border-left:1px solid rgba(0,0,0,0.8);
border-bottom:1px solid rgba(0,0,0,0.3);
height: 17px;}

QListView .QScrollBar::sub-line:vertical
QTreeWidget .QScrollBar::sub-line:vertical,
QTableWidget .QScrollBar::sub-line:vertical,
QMdiArea .QScrollBar::sub-line:vertical,
QGraphicsView .QScrollBar::sub-line:vertical,
QTextEdit .QScrollBar::sub-line:vertical,
QPlainTextEdit.QScrollBar::sub-line:vertical,
QIconView .QScrollBar::sub-line:vertical {
